{
  "name": "AJAX Text Extension",
  "version": "1.0",
  "manifest_version": 3,
  "host_permissions": [
    "<all_urls>"
  ],
  "action":{
    "default_popup": "index.html",
    "default_title": "AJAX Detector"
  },
  "permissions": [
    "unlimitedStorage", 
    "storage",
    "scripting",
    "nativeMessaging",
    "webRequest",
    "activeTab",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  }
}

  

